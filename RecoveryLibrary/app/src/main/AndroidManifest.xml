<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zxy.recovery.test">

    <uses-permission android:name="android.permission.GET_TASKS" />
    <application
        android:name="com.zxy.recovery.test.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.zxy.recovery.test.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.zxy.recovery.test.TestActivity" />
        <activity android:name="com.zxy.recovery.test.TestActivity2" />
    </application>

</manifest>
